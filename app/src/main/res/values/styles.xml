<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.Holo.Light">
        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/customActionBar</item>
    </style>

    <style name="customActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#fec107</item>
        <item name="android:titleTextColor">@style/customColorApperenace</item>
    </style>
    <style name="customColorApperenace" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#000</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="RatingBar"
        parent="android:style/Widget.Material.RatingBar.Small">
        <item name="colorControlNormal">#fff</item>
        <item name="colorControlActivated">#fec107</item>
    </style>


</resources>
